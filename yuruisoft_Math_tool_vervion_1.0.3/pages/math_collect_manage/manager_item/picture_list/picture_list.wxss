page{
    height:100%;
    min-height:100%;
    background: #e9e4d9;
    color:black;
    font-family:"Microsoft Yahei","宋体";
    font-size:14px;
}
._image{
    width: 100rpx;
    height: 100rpx;
}
._image_container{
    width: 100rpx;
    height: 100rpx;
    margin: 20rpx;
}
.image_hove{
    background-color: gray;
    border-radius: 200rpx;
}
.image_title{
    display: flex;
    flex-direction: row;
}
.delete{
    border: 1rpx solid black;
}
.item{
    display: flex;
    flex-direction: column;
    border: 1rpx solid black;
    width: 95%;
    margin: 20rpx;
}
.item_position_data{
    font-size: 10px;
    word-wrap: break-word;
}
.line{
    height: 1px;
    width:100%;
    background-color: #d1d1cf;
}
.container_{
    display: flex;
    flex-direction: row;
}
.check_box{
    margin-left: -200rpx;
}


